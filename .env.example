# Required: OpenAI API key for embeddings
OPENAI_API_KEY=sk-...

# Optional: TraceMem-specific overrides
# TRACEMEM_GRAPH_STORE=kuzu          # kuzu (default) or neo4j
# TRACEMEM_HOME=~/.tracemem          # Storage directory
# TRACEMEM_OPENAI_API_KEY=sk-...     # Override for TraceMem specifically
# TRACEMEM_EMBEDDING_MODEL=text-embedding-3-small
# TRACEMEM_EMBEDDING_DIMENSIONS=1536
# TRACEMEM_RERANKER=rrf              # rrf (default) or linear

# Optional: Neo4j (only if TRACEMEM_GRAPH_STORE=neo4j)
# TRACEMEM_NEO4J_URI=bolt://localhost:7687
# TRACEMEM_NEO4J_USER=neo4j
# TRACEMEM_NEO4J_PASSWORD=password
# TRACEMEM_NEO4J_DATABASE=neo4j
# TRACEMEM_NAMESPACE=                # Multi-user isolation namespace

# Optional: LangSmith tracing
# LANGSMITH_API_KEY=lsv2_pt_...
